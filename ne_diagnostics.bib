@article{Lemal2016,
abstract = {The absolute radiation measurements obtained in the electric arc-driven shock-tube facility at NASA Ames Research Center were analyzed to test the collisional-radiative model developed at cole Centrale Paris. Two conditions representative of Earth reentry at 10.54 and 11.17 km-s were investigated in the vacuum ultraviolet and infrared spectral ranges. For each of the conditions, the corresponding charge-coupled device images were analyzed. The electron number density was inferred from Stark-broadened nitrogen and H lines. Comparisons with the predicted electron number density profiles enabled us to validate the ionization rate constant model implemented in the flowfield solver and to accurately locate the shock front. For both freestream conditions and all the spectral ranges, the predictions of the initial intensity rises were improved when the total spatial smearing (due to the shock motion, the optics, and the camera) was taken into account. The nonequilibrium intensities observed in the vacuum ultraviolet and infrared spectral ranges were underpredicted by the collisional-radiative model when only electron-impact excitation and ionization processes were taken into account. Then, the effect of heavy-particle impact processes was studied by applying various multitemperature dissociation rate constant and vibration-dissociation coupling models as well as heavy-particle impact excitation models. The nonequilibrium peak intensities observed in the vacuum ultraviolet and infrared spectral ranges were shown to be controlled by heavy-particle impact excitation processes.},
author = {Lemal, A. and Jacobs, C. M. and Perrin, M.-Y. and Laux, C. O. and Tran, P. and Raynaud, E.},
doi = {10.2514/1.T4550},
file = {:Users/yangli/Library/Application Support/Mendeley Desktop/Downloaded/Lemal et al. - 2016 - Prediction of Nonequilibrium Air Plasma Radiation Behind a Shock Wave.pdf:pdf;:Users/yangli/Library/Application Support/Mendeley Desktop/Downloaded/Lemal et al. - 2014 - Prediction of Nonequilibrium Air Plasma Radiation Behind a Shock Wave.pdf:pdf},
issn = {0887-8722},
journal = {J. Thermophys. Heat Transf.},
number = {1},
pages = {197--210},
title = {{Prediction of Nonequilibrium Air Plasma Radiation Behind a Shock Wave}},
url = {http://arc.aiaa.org/doi/10.2514/1.T4550 http://arc.aiaa.org},
volume = {30},
year = {2016}
}
@article{Konjevic1999,
abstract = {The present status of the experimental studies of plasma broadening and shifting of non-hydrogenic neutral atom and positive ion spectral lines is discussed with an emphasis to the plasma diagnostic applications. A short overview of the available theoretical results is followed by the description of experimental techniques for the line shape and shift measurements. The influence of other broadening mechanisms to the Stark width and shift determination is discussed and a typical experimental procedure described. To select higher accuracy experimental data for plasma diagnostic purposes the available experimental results are analyzed and the uncertainty of recommended ones is estimated. The procedure for application of selected lines for plasma electron density diagnostics is described. The influence of ion dynamics to the width and shift of visible helium lines in low electron density plasmas is examined. In order to test theories the selected data are compared with results of various Stark width and shift calculations. Neutral atom lines asymmetry and new techniques for ion-broadening parameter measurements are reviewed. The studies of line widths and shifts along isoelectronic sequences of multiply ionized atoms are also discussed. {\textcopyright} 1999 Elsevier Science B.V. All rights reserved.},
author = {Konjevic, N and Konjevich, N and Konjevi{\'{c}}, N.},
doi = {10.1016/S0370-1573(98)00132-X},
file = {:Users/yangli/Downloads/1-s2.0-S037015739800132X-main.pdf:pdf;:Users/yangli/Library/Application Support/Mendeley Desktop/Downloaded/Konjevic, Konjevich - 1999 - PLASMA BROADENING AND SHIFTING OF NON-HYDROGENIC SPECTRAL LINES PRESENT STATUS AND APPLICATIONS.pdf:pdf},
issn = {03701573},
journal = {Phys. Rep.},
keywords = {Electron density diagnostics,Plasma spectroscopy,Stark broadening},
number = {6},
pages = {339--401},
title = {{Plasma broadening and shifting of non-hydrogenic spectral lines: Present status and applications}},
volume = {316},
year = {1999}
}
@article{Huo2012,
abstract = {Based on Baranger's impact theory, the Stark line shape due to the interaction of a radiating particle (atom, molecule, ion) with electrons is studied. A semiclassical approximation is employed to determine the cross sections appearing in the line shape function. This formulation is applied to study the Stark broadening of atomic lines. The present implementation of the semiclassical approximation differs from earlier work in four aspects: The atomic line positions and oscillator strengths are taken from the database in the HyperRad code. Seaton's formula is used to enforce the reciprocity condition in the inelastic cross sections. Higher order correction is incorporated to the cross sections by a scaling factor. Also dynamic polarizability instead of static polarizability is used in the elastic cross section calculations. The Stark width and shift parameters of the N and O atoms are compared with the experimental data from the NIST compilations. In general, agreement in the UV region is better that the longer wavelength region. Also, the shift parameter is more sensitive to the computational method than the width. The shift parameter has not been used previously in flow field radiative heat load calculations. Nomenclature a 0 = Bohr radius a($\beta$) = function appearing in the expression of $\sigma$ ij b = impact parameter b D = Debye length B i , B j , B ij = angular factors in quadrupole interactions c = velocity of light d = dipole moment operator E B = B-scaled energy E IP = Lowest ionization potential of an atomic level E i = Energy of atomic level i E = average electron energy E i = electron energy before collision E j = electron energy after collision f(v) = electron velocity distribution f ij = oscillator strength of the i→f transition F($\omega$) = line shape function i, j, f = eigenstates of the atom g i = degeneracy of state i ! i , = eigenstates of the atom + perturber system I H = Rydberg constant I 0 ($\beta$) = modified Bessel function I of zeroth order I 1 ($\beta$) = modified Bessel function I of first order J i = total angular momentum of the i state k B = Boltzmann constant K 0 ($\beta$) = modified Bessel function K of zeroth order K 1 ($\beta$) = modified Bessel function K of first order m e = electron mass * Principal Scientist. Email: Winifred.Huo@nasa.gov or w.m.huo@sbcglobal.net. 2 m i = projected angular momentum of the i state along the Z-axis M = projected angular momentum of the dipole operator along the Z-axis N e = electron number density N z = number density of zth ion P if = power radiated from a transition from ! i to q e = charge of the electron r a = smaller of the mean radius for the initial and final state = mean atomic radius of level j Re = real part of s = time variable T = temperature T(b,v) = semiclassical T-matrix as a function of the impact parameter b and perturber velocity v v = electron velocity v i = electron velocity before collision v j = electron velocity after collision v = average electron velocity $\delta$ if = Stark shift for i→f transition $\Delta$E ij = energy difference between i and j states $\epsilon$ o = permittivity of free space $\Phi$(s) = autocorrelation function $\gamma$ if = Stark width (half-width at half max) for i→f transition $\phi$ if ($\omega$) = line shape for the i→f transition ϕ p = difference between the elastic phase shifts of the initial and final state due to polarization interaction ϕ q = difference between the elastic phase shifts of the initial and final state due to quadrupole interaction ϕ i→j = elastic phase shift of the i state due to polarization by the j state $\rho$ i = probability of the system in state ! i $\rho$ Ai = probability of the atom in state i $\sigma$ ij = inelastic cross section of i→j transition by electron impact $\sigma$ elas = elastic contribution to the Stark width $\omega$ = energy in angular frequency $\omega$ i = energy of state i in angular frequency $\omega$ if = transition frequency from state i to f.},
