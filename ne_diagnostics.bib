@inproceedings{Grinstead2008,
abstract = {NASA's Crew Exploration Vehicle Aeroscience Project is supporting ground testing for the development of Earth re-entry aerodynamic and aerothermal databases for the Orion crew vehicle. For lunar return, shock layer radiation will constitute a significant portion of the total heat flux at the peak heating point in the entry trajectory. Phenomenological models of nonequilibrium radiation transport will be incorporated into high-fidelity flow field computations used to predict the aerothermal environments for Orion. These models are validated with shock radiance measurements obtained at flight-relevant conditions. A comprehensive test series in the NASA Ames Electric Arc Shock Tube facility at representative conditions was recently completed. The facility optical instrumentation enabled spectral measurements of shocked gas radiation from the ultraviolet to the near infrared. The instrumentation captured the nonequilibrium post-shock excitation and relaxation dynamics of dispersed spectral features. A description of the EAST facility, optical instrumentation, and examples of the test data are presented.},
author = {Grinstead, Jay H. and Wilder, Michael C. and Olejniczak, Joseph and Bogdanoff, David W. and Allen, Gary A. and Dang, Kim and Forres, Michael J.},
