issn = {00224073},
journal = {J. Quant. Spectrosc. Radiat. Transf.},
number = {6},
pages = {621--633},
title = {{Semiconductor laser absorption diagnostics of atomic oxygen in an atmospheric-pressure plasma}},
volume = {50},
year = {1993}
}
